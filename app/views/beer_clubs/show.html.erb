<p id="notice"><%= notice %></p>

<h1>
  <%= @beer_club.name %>
  <%= @beer_club.city %>
</h1>

<p>
Founded
  <%= @beer_club.founded %>
</p>

<% if @beer_club.members.empty? %>
  No members yet
<% else %>
  <ul>
    <% @beer_club.members.each do |member| %>
	<li><%= link_to member.to_s, user_path(member) %></li>
    <% end %>
  </ul>
<% end %>
    



<%= link_to 'Edit', edit_beer_club_path(@beer_club) %> |
<%= link_to 'Back', beer_clubs_path %>
